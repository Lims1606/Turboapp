<%= form_with model: resource, url: registration_path(resource_name), data: { turbo_stream: true } do |f| %>
  <div>
    <%= f.label :email %><br>
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.label :password_confirmation %><br>
    <%= f.password_field :password_confirmation %>
  </div>

  <div>
    <%= f.submit "Sign up" %>
  </div>
<% end %>
